&mtp_batterydata {
	#include "battery/LC_M6300_ATL4000mAh_3951184_FG.dtsi"
	#include "battery/LC_M6300_Coslight_4000mAh_3884533_FG.dtsi"
	#include "battery/LC_M6300_Sunwoda4000mAh_3632066_FG.dtsi"
};

&pm6150_qg {
	qcom,cl-disable = <1>;
	qcom,qg-iterm-ma = <350>;
	qcom,s3-entry-fifo-length = <2>;
	qcom,s3-entry-ibat-ua = <30000>;
	qcom,s3-exit-ibat-ua = <50000>;
	qcom,vbatt-empty-cold-mv = <3100>;
	qcom,vbatt-empty-mv = <3100>;
	qcom,vbatt-low-cold-mv = <3700>;
	qcom,vbatt-low-mv = <3300>;
};

&pm6150_charger {
	io-channels = <&pm6150_vadc ADC_USB_IN_V_16>,
		      <&pm6150_vadc ADC_USB_IN_I>,
		      <&pm6150_vadc ADC_CHG_TEMP>,
		      <&pm6150_vadc ADC_DIE_TEMP>,
		      <&pm6150_vadc ADC_SBUx>,
		      <&pm6150_vadc ADC_VPH_PWR>;
	io-channel-names = "usb_in_voltage",
			   "usb_in_current",
			   "chg_temp",
			   "die_temp",
			   "sbux_res",
			   "vph_voltage";
	google,usb-port-tz-name = "usbc-therm-adc";
	qcom,chg-term-current-ma = <(-200)>;
	qcom,chg-term-src = <1>;
	qcom,dc-icl-ua = <100>;
	qcom,sec-charger-config = <0>;
	qcom,auto-recharge-soc = <99>;
	qcom,thermal-mitigation = <3000000 1850000 925000
			500000>;
};
